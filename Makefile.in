
SRC = scgi-pie.c pie.c buffer.c
HEADERS = buffer.h scgi-pie.h

INSTALL?=install
CFLAGS=@@CFLAGS@@
LDFLAGS=@@LDFLAGS@@

OBJ = $(SRC:.c=.o)

@@NAME@@: $(OBJ)
	$(CC) $(OBJ) $(LDFLAGS) -o $@

install:
	$(INSTALL) -s @@NAME@@ @@BINDIR@@/@@INSTALL_NAME@@
	@@LN_COMMAND@@

clean:
	rm -f @@NAME@@ $(OBJ)

.PHONY: clean install
